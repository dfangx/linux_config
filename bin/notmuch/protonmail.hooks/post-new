#!/bin/sh
export NOTMUCH_CONFIG=/home/cyrusng/.config/notmuch/protonmail-notmuch

notmuch tag -inbox +sent folder:Sent
notmuch tag -inbox +archive folder:Archive
notmuch tag -inbox +draft folder:Drafts
notmuch tag -inbox +spam folder:Spam
notmuch tag -inbox +deleted folder:Trash
notmuch tag +starred folder:Starred
